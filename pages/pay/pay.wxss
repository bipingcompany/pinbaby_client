.payList {
  padding: 30rpx 30rpx 184rpx;
}
.listItem {
  margin-bottom: 30rpx;
  background: #fff;
  padding: 40rpx 30rpx;
}
.listItem .title {
  display: block;
  font-size: 34rpx;
  color: #333;
}
.listItem .content {
  display: block;
  font-size: 28rpx;
  color: #999;
}
.payFooter {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 184rpx;
  background: #ffffff;
  text-align: center;
}
.payFooter .price {
  margin: 14rpx 0;
  font-size: 32rpx;
  color: #666666;
}
.payFooter .price .num {
  color: #FE5858;
}
.payFooter .payBtn {
  width: 690rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #50D27D;
  color: #fff;
  margin: 0 auto;
  border-radius: 44rpx;
}
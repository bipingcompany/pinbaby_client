.detail .banner {
  width: 100%;
  vertical-align: bottom;
}
.detail .detailInfo {
  position: relative;
  padding: 30rpx;
  margin-bottom: 20rpx;
  background: #fff;
}
.detail .detailInfo .price {
  color: #FE5858;
  font-size: 40rpx;
}
.detail .detailInfo .people {
  display: inline-block;
  height: 32rpx;
  line-height: 32rpx;
  border: 1px solid #FE5858;
  color: #FE5858;
  font-size: 24rpx;
  padding: 0 14rpx;
  border-radius: 21rpx;
  margin-left: 20rpx;
}
.peopleImg {
  width: 25rpx;
  height: 21rpx;
}
.detail .detailInfo .shareBtn {
  width: 76rpx;
  height: 36rpx;
  float: right;
  background: url("data:image/jpeg;base64,") no-repeat center;
  background-size: cover;
  font-size: 24rpx;
  color: #666666;
  text-align: center;
  line-height: 36rpx;
  margin: 0;
  padding: 0;
}
.detail .detailInfo .title {
  display: block;
  color: #333333;
  font-size: 34rpx;
}
.detail .detailInfo .tag .tagItem{
  height: 42rpx;
  background: #FEEEEE;
  color: #FE5858;
  border-radius: 21rpx;
  font-size: 24rpx;
  padding: 5rpx 14rpx;
}
.activityImg {
  width: 32rpx;
  height: 35rpx;
}
.content {
  background: #fff;
  padding: 40rpx 30rpx 182rpx;
}
.content .title {
  color: #333333;
  font-size: 32rpx;
}
.content .con{
  color: #666666;
  font-size: 28rpx;
  line-height: 1.5;
  white-space: pre-line;
  text-align: justify;
}
.footer {
  display: flex;
  align-items: center;
  justify-items: center;
  position: fixed;
  bottom: 0;
  height: 142rpx;
  border-top: 1px solid #ECECEC;
  background: #fff;
  width: 100%;
  justify-items: center;
}
.footer .goIndex {
  flex: 0 0 108rpx;
  text-align: center;
  font-size: 20rpx;
  color: #999999;
}
.footer .goIndex .homeImg {
  width: 36rpx;
  height: 45rpx;
  display: block;
  margin: 0 auto;
}
.footer .singleBuy {
  flex: 0 0 248rpx;
}
.footer .singleBuy button {
  width: 248rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #FFAB2A;
  color:#FFFFFF;
  font-size: 28rpx;
  border-radius: 44rpx;
}
.footer .groupBuy {
  flex: 1;
}
.footer .groupBuy button {
  width: 326rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #FE5858;
  color:#FFFFFF;
  font-size: 28rpx;
  border-radius: 44rpx;
}